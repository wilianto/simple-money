.container
  .row
    .col-md-4.col-md-offset-4
      h1 Sign In
      = form_for :session, url: login_path do |f|
        = f.label :email
        = f.email_field :email, class: "form-control"

        = f.label :password
        = f.password_field :password, class: "form-control"

        = f.submit "Login", class: "btn btn-primary btn-block"
